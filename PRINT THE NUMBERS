#include<stdio.h>
#include<conio.h>
int main()
{
int  
printrow,col,n,space,n;
printf("enter the nu of rows\n");
scanf("%d",&n);
for(row=1;row<=n:row++)
{
for(space=1;space<=n-row;space++)
{
printf("");
}
for (col=1;col<=row:col++);
{
for(i=1;i<=n;i++);
{
printf(%d,"i");
}
printf(\n);
}
getch();
return 0;
